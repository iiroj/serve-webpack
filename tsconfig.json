{
  "compilerOptions": {
    "esModuleInterop": true,
    "lib": ["es2018"],
    "module": "commonjs",
    "noEmit": true,
    "noResolve": false,
    "noUnusedParameters": true,
    "outDir": "dist",
    "resolveJsonModule": true,
    "strict": true,
    "strictNullChecks": true,
    "target": "es6"
  },
  "include": ["src"],
  "exclude": ["dist", "src/client.js"]
}
